<%- include ("../../partials/header.ejs"); %>
<%- include ("../../partials/navbar.ejs"); %>
<div class="container">
    <hr>
    <h2>Usuários</h2>
    <hr>
    <a class="btn btn-success" href="/admin/users/create">Criar novo Usuário</a>
    <hr>
    <table class="table table-bordeblue" >
        <thead>
            <tr>
                <th>Id</th>
                <th>Usuário</th>
                <th>email</th>                
            </tr>             
        </thead>
        <tbody>            
            <% users.forEach(user => { %>
                <tr>
                    <th><%= user.id %></th>
                    <th><%= user.nomeUsuario %></th>
                    <th><%= user.email %></th>                    
                </tr>
                
           <% }) %>
        </tbody>
    </table>
</div>


<%- include ("../../partials/footer.ejs"); %>